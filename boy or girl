#include <iostream>
#include <bits/stdc++.h>

using namespace std;

int main()
{
string s;
int x=0,c=0,y=0;
cin>>s;
x=s.size();
sort(s.begin(),s.end());


    for(int j=0;j<x;j++)

        if(s[j]==s[j+1])
            c++;

y=x-c;
if(y%2==0)
    cout<<"CHAT WITH HER!"<<endl;
else
    cout<<"IGNORE HIM!"<<endl;

    return 0;
}
